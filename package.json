{
  "name": "travel-booking-system-backend",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "nodemon",
    "build": "tsc -p tsconfig.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/database/connections/index.ts",
    "schema:drop": "npm run typeorm schema:drop",
    "m:generate": "npm run typeorm migration:generate ./src/database/migrations/%npm_config_name%",
    "m:run": "npm run typeorm migration:run",
    "m:revert": "npm run typeorm migration:revert",
    "m:fresh": "npm run typeorm schema:drop && npm run typeorm migration:run",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/config/ormconfig.ts"
  },
  "keywords": [],
  "author": "tommycontreras34@gmail.com",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "20.14.8",
    "express": "^4.19.2",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "bcrypt": "5.1.1",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "config": "3.3.11",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "3.10.1",
    "reflect-metadata": "0.2.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.20",
    "typeorm-seeding": "1.6.1",
    "winston": "3.13.0"
  }
}
